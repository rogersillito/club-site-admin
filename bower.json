{
  "name": "club-site-admin",
  "description": "CMS and web front-end to drive a sports club website",
  "main": "keystone.js",
  "authors": [
    "rogersillito <rogersillito@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/rogersillito/club-site-admin",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "spec"
  ],
  "dependencies": {
    "jquery": "^1.12.3",
    "react": "^15.0.1",
    "babel": "*",
    "bootstrap-sass": "^3.3.6",
    "es6-promise": "^3.2.1",
    "fetch": "whatwg-fetch#^1.0.0",
    "smartmenus": "^1.0.0",
    "bootstrap": "^3.3.6"
  },
  "exportsOverride": {
    "bootstrap-sass": {
      "js": "assets/javascripts/*.js",
      "styles": "assets/stylesheets/*.scss",
