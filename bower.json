      "styles/bootstrap/mixins": "assets/stylesheets/bootstrap/mixins/*.scss",
      "fonts": "assets/fonts/bootstrap/*.*"
    },
    "bootswatch": {
      "/": "journal/*.scss"
    }
  },
  "devDependencies": {
    "bootswatch": "^3.3.6+1"
  }
